   "id": "bd5d91ea-e429-4f26-ae67-03c3854ff235",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        l = s.split(' ')\n",
    "        res = []\n",
    "        for w in l:\n",
    "            res.append(w[::-1])\n",
    "        \n",
    "        return ' '.join(res)"
   ]
